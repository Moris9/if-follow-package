{
  "source": {
    "include": ["./src/"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "encoding": "utf8",
    "private": true,
    "recurseDepth": 10,
    "sourceType": "module"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "plugins": []
}
